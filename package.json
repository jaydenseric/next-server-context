{
  "name": "next-server-context",
  "version": "2.0.0",
  "description": "A Next.js App or page decorator, React context object, and React hook to access Node.js HTTP server context when rendering components.",
  "license": "MIT",
  "author": {
    "name": "Jayden Seric",
    "email": "me@jaydenseric.com",
    "url": "https://jaydenseric.com"
  },
  "repository": "github:jaydenseric/next-server-context",
  "homepage": "https://github.com/jaydenseric/next-server-context#readme",
  "bugs": "https://github.com/jaydenseric/next-server-context/issues",
  "funding": "https://github.com/sponsors/jaydenseric",
  "keywords": [
    "next",
    "server",
    "context",
    "request",
    "response",
    "react",
    "hooks",
    "ssr",
    "esm",
    "mjs"
  ],
  "files": [
    "public"
  ],
  "sideEffects": false,
  "main": "public",
  "exports": {
    ".": {
      "import": "./public/index.mjs",
      "require": "./public/index.js"
    },
    "./public/": "./public/",
    "./package": "./package.json",
    "./package.json": "./package.json"
  },
  "engines": {
    "node": "^12.20 || >= 14.13"
  },
  "browserslist": "Node 12.20 - 13 and Node < 13, Node >= 14.13, > 0.5%, not OperaMini all, not IE > 0, not dead",
  "peerDependencies": {
    "next": "9.0.3 - 10",
    "react": "16.14 - 17"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.7.0",
    "eslint": "^7.13.0",
    "eslint-config-env": "^15.0.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jsdoc-md": "^11.0.0",
    "next": "^10.0.1",
    "prettier": "^2.1.2",
    "react": "^17.0.1",
    "size-limit": "^4.7.0"
  },
  "scripts": {
    "jsdoc": "jsdoc-md",
    "test": "npm run test:eslint && npm run test:prettier && npm run test:jsdoc && npm run test:size",
    "test:eslint": "eslint --ext mjs,js .",
    "test:prettier": "prettier -c .",
    "test:jsdoc": "jsdoc-md -c",
    "test:size": "size-limit",
    "prepublishOnly": "npm test"
  }
}
